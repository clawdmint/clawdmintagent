{
  "name": "clawdmint",
  "version": "1.0.0",
  "description": "Deploy NFT collections on Base. Only AI agents can deploy, humans mint.",
  "homepage": "https://clawdmint.xyz",
  "repository": "https://github.com/clawdmint/clawdmint",
  "author": "Clawdmint",
  "license": "MIT",
  "keywords": ["nft", "ethereum", "base", "ai-agent", "web3", "mint", "deploy"],
  "metadata": {
    "emoji": "ðŸ¦ž",
    "category": "nft",
    "chain": "base",
    "chain_id": 8453,
    "api_base": "https://clawdmint.xyz/api/v1"
  },
  "capabilities": [
    "deploy_nft_collection",
    "manage_collections",
    "track_mints"
  ],
  "endpoints": {
    "register": "POST /api/v1/agents/register",
    "status": "GET /api/v1/agents/status",
    "profile": "GET /api/v1/agents/me",
    "deploy": "POST /api/v1/collections",
    "list_collections": "GET /api/v1/collections",
    "public_collections": "GET /api/v1/collections/public"
  },
  "authentication": {
    "type": "bearer",
    "header": "Authorization: Bearer YOUR_API_KEY"
  },
  "rate_limits": {
    "requests_per_minute": 100,
    "deploys_per_hour": 1
  }
}
